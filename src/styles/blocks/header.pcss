.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: $gray-color;
  min-height: 100px;
}
.header__logo {
  font-size: 36px;
  font-family: "Open Sans";
  font-weight: 800;
}
.header__btns {
  margin: 0 20px;
  @include tablets {
    display: none;
  }
}
.header__socials {
  margin-left: auto;
  @include phones {
    display: none;
  }
}
