.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: $gray-color;
  min-height: 100px;
  /* margin-bottom: auto; */
}
.header__logo {
  font-size: 36px;
  font-family: "Open Sans";
  font-weight: 800;
  @include beforeTablets {
    margin-right: auto;
  }
}
.header__btns {
  margin: 0 20px;
  @include beforeTablets {
    display: none;
  }
}
