.rv-slider__list {
  display: flex;
}
.rv-slider__item {
  display: flex;
  width: 38%;
  flex-shrink: 0;

  &:nth-child(2n + 1) {
    margin-right: 4%;
  }
  @include tablets {
    width: 50%;
  }
}
.rv-slider__item-quote {
  margin-right: 20px;
  
}
.rv-slider__item-content {
  font-family: "Open Sans";
}
